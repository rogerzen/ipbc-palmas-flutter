  "verses": [
    {
      "id": "0",
      "isChorus": false,
      "versesList": [
        {
         "verse0": "Se nos cega o sol ardente",
         "verse1": "Quando visto em seu fulgor,",
         "verse2": "Quem contemplará Aquele",
         "verse3": "Que do sol é Criador?",
         "verse4": "Patriarcas não puderam",
         "verse5": "O seu rosto contemplar,",
         "verse6": "Nem Adão chegou a vê-lo",
         "verse7": "Antes mesmo de pecar!"
        }
      ]
    },
    {
      "id": "1",
      "isChorus": true,
      "versesList": [
        {
         "verse0": "Luz perante a qual é trevas",
         "verse1": "Mesmo o sol a fulgurar!",
         "verse2": "Nossos olhos pecadores",
         "verse3": "Não te podem contemplar!",
         "verse4": "Fogo em cima da arca santa,",
         "verse5": "Sarça ardente do Sinai",
         "verse6": "São figuras desta glória",
         "verse7": "Do Senhor e Eterno Pai."
        }
      ]
    },
    {
      "id": "2",
      "isChorus": true,
      "versesList": [
        {
         "verse0": "Para termos nós com ele",
         "verse1": "Franca e doce comunhão,",
         "verse2": "Cristo, o Filho fez-se carne,",
         "verse3": "Fez-se nossa Redenção!",
         "verse4": "Para que na glória eterna",
         "verse5": "O vejamos já sem véu,",
         "verse6": "Cristo padeceu a morte,",
         "verse7": "O caminho abrindo ao céu."
        }
      ]
    }
  ]
